    <item android:left="0dp" android:right="0dp" android:top="0dp" android:bottom="0dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dp" android:color="@color/darkGray" />
        </shape>
    </item>
-->
</layer-list>