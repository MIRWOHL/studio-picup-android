<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/username_icon" />


    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/darkGreen" />
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="0dp" android:topRightRadius="0dp" />
            <padding android:top="4dp" android:left="4dp" android:bottom="4dp" android:right="4dp" />


        </shape>
    </item>


    <item>
        <shape android:shape="oval" android:innerRadius="30dp">
            <stroke android:color="@color/darkGray" android:width="1dp"/>
            <solid android:color="@android:color/transparent" />

        </shape>
    </item>
